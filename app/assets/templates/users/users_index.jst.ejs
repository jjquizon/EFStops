<div class="backbone-temp col-xs-8 col-xs-offset-2">
  <div class="row">
    <h1>List of Users</h1>
  </div>

  <div class="row">
    <ul class="list-unstyled">
      <% users.forEach(function (user, idx) { %>
        <% idx += 1 %>

        <span class ="col-xs-3 user-index">
          <a href="#/users/<%= user.id %>">
            <img src="<%= user.escape('filepicker_url') %>" class="avatar-size"/>
            <%= user.escape('username') %>
          </a>
        </span>
        <% if (idx % 4 === 0 && idx != 0) { %>
          <br>
        <% } %>
      <% }); %>
    </ul>
  </div>
</div>
